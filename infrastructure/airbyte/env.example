# Copy to .env and source before running docker compose to keep secrets out of git.
# All values are redacted placeholders.

# -----------------------------------------------------------------------------
# Airbyte API / Compose endpoints
# -----------------------------------------------------------------------------
AIRBYTE_BASE_URL=http://localhost:8001
AIRBYTE_API_AUTH_HEADER=

# db
AIRBYTE_DB_USER=airbyte
AIRBYTE_DB_PASSWORD=airbyte_password
AIRBYTE_DB_NAME=airbyte

# server + webapp ports
AIRBYTE_SERVER_PORT=8001
AIRBYTE_WEBAPP_PORT=8000

# default timezone + scheduling hints
AIRBYTE_DEFAULT_TIMEZONE=America/Jamaica
AIRBYTE_DEFAULT_METRICS_CRON=0 6-22 * * *
AIRBYTE_DEFAULT_METRICS_TIMEZONE=America/Jamaica
AIRBYTE_DEFAULT_METRICS_BASIC_UNITS=1
AIRBYTE_DEFAULT_METRICS_BASIC_TIME_UNIT=hours
AIRBYTE_DEFAULT_DAILY_CRON=15 2 * * *
AIRBYTE_DEFAULT_DAILY_TIMEZONE=America/Jamaica
AIRBYTE_DEFAULT_DAILY_BASIC_UNITS=1
AIRBYTE_DEFAULT_DAILY_BASIC_TIME_UNIT=days

# reusable destination hints
AIRBYTE_DEFAULT_STREAM_PREFIX=
AIRBYTE_DEFAULT_DESTINATION_NAMESPACE=analytics
AIRBYTE_DEFAULT_DESTINATION_BUCKET=s3://placeholder-bucket

# -----------------------------------------------------------------------------
# Connection templates used when cloning new tenants
# Provide the UUID of a "golden" connection per connector so bootstrap scripts
# can copy sync catalogs and operation chains.
# -----------------------------------------------------------------------------
AIRBYTE_TEMPLATE_META_METRICS_CONNECTION_ID=11111111-1111-1111-1111-111111111111
AIRBYTE_TEMPLATE_GOOGLE_METRICS_CONNECTION_ID=22222222-2222-2222-2222-222222222222
AIRBYTE_TEMPLATE_DIMENSIONS_DAILY_CONNECTION_ID=33333333-3333-3333-3333-333333333333

# -----------------------------------------------------------------------------
# Tenants: comma-separated list of slugs used to derive env variable names.
# Each slug may be overridden with DISPLAY_NAME to control generated connection
# names. Provide workspace + destination identifiers per tenant.
# -----------------------------------------------------------------------------
AIRBYTE_TENANTS=tenant_alpha

# Tenant Alpha configuration
AIRBYTE_TENANT_ALPHA_DISPLAY_NAME=Tenant Alpha
AIRBYTE_TENANT_ALPHA_WORKSPACE_ID=aaaaaaaa-aaaa-aaaa-aaaa-aaaaaaaaaaaa
AIRBYTE_TENANT_ALPHA_DESTINATION_ID=bbbbbbbb-bbbb-bbbb-bbbb-bbbbbbbbbbbb
AIRBYTE_TENANT_ALPHA_DESTINATION_NAMESPACE=tenant_alpha_analytics
AIRBYTE_TENANT_ALPHA_DESTINATION_BUCKET=s3://tenant-alpha-landing
AIRBYTE_TENANT_ALPHA_STREAM_PREFIX=tenant_alpha_

# Schedules (override defaults per tenant or keep blank to use global values)
AIRBYTE_TENANT_ALPHA_METRICS_CRON=0 6-22 * * *
AIRBYTE_TENANT_ALPHA_METRICS_TIMEZONE=America/Jamaica
AIRBYTE_TENANT_ALPHA_METRICS_BASIC_UNITS=1
AIRBYTE_TENANT_ALPHA_METRICS_BASIC_TIME_UNIT=hours
AIRBYTE_TENANT_ALPHA_DAILY_CRON=15 2 * * *
AIRBYTE_TENANT_ALPHA_DAILY_TIMEZONE=America/Jamaica
AIRBYTE_TENANT_ALPHA_DAILY_BASIC_UNITS=1
AIRBYTE_TENANT_ALPHA_DAILY_BASIC_TIME_UNIT=days

# Optional per-connection overrides (names, prefixes, status, or pre-existing IDs)
AIRBYTE_TENANT_ALPHA_META_METRICS_NAME=Tenant Alpha Meta Metrics
AIRBYTE_TENANT_ALPHA_META_METRICS_PREFIX=meta_
AIRBYTE_TENANT_ALPHA_META_METRICS_STATUS=active
# AIRBYTE_TENANT_ALPHA_META_METRICS_CONNECTION_ID=44444444-4444-4444-4444-444444444444

AIRBYTE_TENANT_ALPHA_GOOGLE_METRICS_NAME=Tenant Alpha Google Metrics
AIRBYTE_TENANT_ALPHA_GOOGLE_METRICS_PREFIX=google_
AIRBYTE_TENANT_ALPHA_GOOGLE_METRICS_STATUS=active
# AIRBYTE_TENANT_ALPHA_GOOGLE_METRICS_CONNECTION_ID=55555555-5555-5555-5555-555555555555

AIRBYTE_TENANT_ALPHA_DIMENSIONS_DAILY_NAME=Tenant Alpha Dimensions Daily
AIRBYTE_TENANT_ALPHA_DIMENSIONS_DAILY_PREFIX=dimensions_
AIRBYTE_TENANT_ALPHA_DIMENSIONS_DAILY_STATUS=active
# AIRBYTE_TENANT_ALPHA_DIMENSIONS_DAILY_CONNECTION_ID=66666666-6666-6666-6666-666666666666

# Backwards compatibility: default workspace for health checks/scripts that have
# not yet migrated to the multi-tenant loader. Point to the primary tenant.
AIRBYTE_WORKSPACE_ID=${AIRBYTE_TENANT_ALPHA_WORKSPACE_ID}

# -----------------------------------------------------------------------------
# Source credentials - replace with real secrets in your private .env
# -----------------------------------------------------------------------------
# meta_source
AIRBYTE_META_ACCOUNT_ID=act_1234567890
AIRBYTE_META_ACCESS_TOKEN=EAABsbCS1iHgBA...
AIRBYTE_META_APP_ID=000000000000000
AIRBYTE_META_APP_SECRET=replace-me
AIRBYTE_META_INSIGHTS_LOOKBACK_DAYS=28
AIRBYTE_META_HOURLY_WINDOW_DAYS=3

# google_ads_source
AIRBYTE_GOOGLE_ADS_DEVELOPER_TOKEN=INSERT_DEVELOPER_TOKEN
AIRBYTE_GOOGLE_ADS_CLIENT_ID=1234567890-abcdef.apps.googleusercontent.com
AIRBYTE_GOOGLE_ADS_CLIENT_SECRET=replace-me
AIRBYTE_GOOGLE_ADS_REFRESH_TOKEN=replace-me
AIRBYTE_GOOGLE_ADS_CUSTOMER_ID=1234567890
AIRBYTE_GOOGLE_ADS_LOGIN_CUSTOMER_ID=1234567890
# Optional: override the default incremental metrics query
# AIRBYTE_GOOGLE_ADS_CUSTOM_QUERY=$(cat infrastructure/airbyte/sources/google_ads_daily_metrics.sql)

# linkedin_transparency_stub (optional)
AIRBYTE_LINKEDIN_CLIENT_ID=REDACTED
AIRBYTE_LINKEDIN_CLIENT_SECRET=REDACTED
AIRBYTE_LINKEDIN_REFRESH_TOKEN=REDACTED

# tiktok_transparency_stub (optional)
AIRBYTE_TIKTOK_TRANSPARENCY_TOKEN=REDACTED
AIRBYTE_TIKTOK_ADVERTISER_ID=REDACTED
