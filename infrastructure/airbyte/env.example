# Copy to .env and source before running docker compose to keep secrets out of git.
# All values are redacted placeholders.

# db
AIRBYTE_DB_USER=airbyte
AIRBYTE_DB_PASSWORD=airbyte_password
AIRBYTE_DB_NAME=airbyte

# server
AIRBYTE_SERVER_PORT=8001
AIRBYTE_WORKSPACE_ID=00000000-0000-0000-0000-000000000000

# webapp
AIRBYTE_WEBAPP_PORT=8000

# meta_source
AIRBYTE_META_ACCOUNT_ID=act_1234567890
AIRBYTE_META_ACCESS_TOKEN=EAABsbCS1iHgBA...
AIRBYTE_META_APP_ID=000000000000000
AIRBYTE_META_APP_SECRET=replace-me
AIRBYTE_META_INSIGHTS_LOOKBACK_DAYS=28
AIRBYTE_META_HOURLY_WINDOW_DAYS=3

# google_ads_source
AIRBYTE_GOOGLE_ADS_DEVELOPER_TOKEN=INSERT_DEVELOPER_TOKEN
AIRBYTE_GOOGLE_ADS_CLIENT_ID=1234567890-abcdef.apps.googleusercontent.com
AIRBYTE_GOOGLE_ADS_CLIENT_SECRET=replace-me
AIRBYTE_GOOGLE_ADS_REFRESH_TOKEN=replace-me
AIRBYTE_GOOGLE_ADS_CUSTOMER_ID=1234567890
AIRBYTE_GOOGLE_ADS_LOGIN_CUSTOMER_ID=1234567890
# Optional: override the default incremental metrics query
# AIRBYTE_GOOGLE_ADS_CUSTOM_QUERY=$(cat infrastructure/airbyte/sources/google_ads_daily_metrics.sql)

# linkedin_transparency_stub (optional)
AIRBYTE_LINKEDIN_CLIENT_ID=REDACTED
AIRBYTE_LINKEDIN_CLIENT_SECRET=REDACTED
AIRBYTE_LINKEDIN_REFRESH_TOKEN=REDACTED

# tiktok_transparency_stub (optional)
AIRBYTE_TIKTOK_TRANSPARENCY_TOKEN=REDACTED
AIRBYTE_TIKTOK_ADVERTISER_ID=REDACTED
