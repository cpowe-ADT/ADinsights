{
  "name": "Meta Marketing (Incremental Template)",
  "sourceType": "facebook-marketing",
  "workspaceId": "00000000-0000-0000-0000-000000000000",
  "configuration": {
    "account_id": "${AIRBYTE_META_ACCOUNT_ID}",
    "access_token": "${AIRBYTE_META_ACCESS_TOKEN}",
    "app_id": "${AIRBYTE_META_APP_ID}",
    "app_secret": "${AIRBYTE_META_APP_SECRET}",
    "start_date": "2024-01-01T00:00:00Z",
    "end_date": null,
    "page_size": 100,
    "include_deleted": true,
    "breakdowns": [
      "country",
      "region",
      "dma"
    ],
    "insights_lookback_window": "${AIRBYTE_META_INSIGHTS_LOOKBACK_DAYS}",
    "window_in_days": "${AIRBYTE_META_HOURLY_WINDOW_DAYS}",
    "attribution_window": "default",
    "fetch_thumbnail_images": false,
    "custom_insights_fields": [
      "impressions",
      "clicks",
      "spend",
      "actions",
      "action_values"
    ]
  },
  "streams": [
    {
      "name": "ad_insights",
      "sync_mode": "incremental",
      "cursor_field": "updated_time",
      "primary_key": [
        "ad_id",
        "updated_time"
      ],
      "destination_sync_mode": "append_dedup"
    },
    {
      "name": "ads",
      "sync_mode": "incremental",
      "cursor_field": "updated_time",
      "primary_key": [
        "id"
      ],
      "destination_sync_mode": "append_dedup"
    },
    {
      "name": "ad_sets",
      "sync_mode": "incremental",
      "cursor_field": "updated_time",
      "primary_key": [
        "id"
      ],
      "destination_sync_mode": "append_dedup"
    },
    {
      "name": "campaigns",
      "sync_mode": "incremental",
      "cursor_field": "updated_time",
      "primary_key": [
        "id"
      ],
      "destination_sync_mode": "append_dedup"
    }
  ]
}
