{
  "name": "Meta Marketing (Incremental Template)",
  "sourceType": "facebook-marketing",
  "workspaceId": "REPLACE_WITH_WORKSPACE_ID",
  "configuration": {
    "account_id": "act_<PRIMARY_ACCOUNT_ID>",
    "access_token": "${AIRBYTE_META_ACCESS_TOKEN}",
    "app_id": "${AIRBYTE_META_APP_ID}",
    "app_secret": "${AIRBYTE_META_APP_SECRET}",
    "start_date": "2024-01-01T00:00:00Z",
    "end_date": null,
    "page_size": 100,
    "include_deleted": true,
    "breakdowns": [
      "country",
      "region",
      "dma"
    ],
    "insights_lookback_window": 3,
    "window_in_days": 28,
    "attribution_window": "default",
    "fetch_thumbnail_images": false,
    "custom_insights_fields": [
      "impressions",
      "clicks",
      "spend",
      "actions",
      "action_values"
    ]
  },
  "streams": [
    {
      "name": "ad_insights",
      "syncMode": "incremental",
      "cursorField": "updated_time",
      "primaryKey": [
        "ad_id",
        "updated_time"
      ]
    },
    {
      "name": "ads",
      "syncMode": "incremental",
      "cursorField": "updated_time",
      "primaryKey": [
        "id"
      ]
    },
    {
      "name": "ad_sets",
      "syncMode": "incremental",
      "cursorField": "updated_time",
      "primaryKey": [
        "id"
      ]
    },
    {
      "name": "campaigns",
      "syncMode": "incremental",
      "cursorField": "updated_time",
      "primaryKey": [
        "id"
      ]
    }
  ]
}
