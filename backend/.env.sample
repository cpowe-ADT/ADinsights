AIRBYTE_WEBHOOK_SECRET=change-me-rotate-per-runbook
# Require webhook secret in non-local environments.
AIRBYTE_WEBHOOK_SECRET_REQUIRED=1

# Optional dev admin (local only; requires DEBUG=True or ALLOW_DEFAULT_ADMIN=1)
ALLOW_DEFAULT_ADMIN=0
